#!/bin/shecho "#> sysINF  ##> Default    $(sysINF)  ###> Operating System    -o            =   $(sysINF -o)    --os          =   $(sysINF --os)  ###> Type    -t            =   $(sysINF -t)    --tpye        =   $(sysINF --tpye)  ###> Kernel    -k            =   $(sysINF -k)    --kernel      =   $(sysINF --kernel)  ###> Architecture    -a            =   $(sysINF -a)    --arch        =   $(sysINF --arch)  ###> Host    -h            =   $(sysINF -h)    --host        =   $(sysINF --host)  ###> User    -u            =   $(sysINF -u)    --user        =   $(sysINF --user)  ###> Shell    -s            =   $(sysINF -s)    --shell       =   $(sysINF --shell)  ###> Window Manager    -w            =   $(sysINF -w)    --wm          =   $(sysINF --wm)"