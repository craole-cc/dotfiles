#!/bin/sh# shellcheck disable=SC2154echo "# ENVIRONMENT VARIABLES  ## sysINF    sys_OS      =>  $sys_OS    sys_TYPE    =>  $sys_TYPE    sys_KERNEL  =>  $sys_KERNEL    sys_ARCH    =>  $sys_ARCH    sys_HOST    =>  $sys_HOST    sys_USER    =>  $sys_USER    sys_SHELL   =>  $sys_SHELL    sys_WM      =>  $sys_WM    sys_POWER   =>  $sys_POWER    term_width  =>  $term_width  ## XMONAD    XMONAD_CONFIG_DIR   =>  $XMONAD_CONFIG_DIR    XMONAD_DATA_DIR     =>  $XMONAD_DATA_DIR    XMONAD_CACHE_DIR    =>  $XMONAD_CACHE_DIR"