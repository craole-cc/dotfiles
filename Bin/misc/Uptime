#!/bin/sh

__uptime__() {
	# echo "remember SED"
	wmic os get LastBootUpTime |
		sed -e "/L/ d" \
			-e "s/\..*//" \
			-e "s/.\{2\}$//"
}

__timeBoot__() {
	wmic os get LastBootUpTime | sed -e "/L/ d"
}

__timeNow__() {
	date +"%Y%m%d%H%M%s"
}

dtdiff "$(__timeNow__)" "$(__timeBoot__)"
