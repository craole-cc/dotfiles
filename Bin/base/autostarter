#!/bin/sh
{ [ "${WAYLAND_DISPLAY}" ] || [ "${DISPLAY}" ]; } || exit

#/> Wayland vs X11
if [ "${WAYLAND_DISPLAY}" ]; then
  launch foot --server
else
  launch picom --config "$DOTS_TOOL/utilities/picom/picom.conf"
fi
dispScreen
launch cbatticon --icon-type standard "$(
  find /sys/class/power_supply | grep BAT
)"
launch blueman-applet
launch volumeicon
launch nm-applet --indicator
launch dunst -config "$DOTS_TOOL/utilities/dunst/dunstrc"

launch flameshot
launch udiskie
launch copyq
# launch doublecmd
# launch firefox
# launch code "$codeDOTS"--
sysMonitor
