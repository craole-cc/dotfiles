#!/bin/sh
# shellcheck disable=SC1091

__init__() {
  #| Load Environment Variables
  . "$DOTS_ENV_CTX/macchina"

  #| Push to $CONFIG_HOME
  symLink --src "$ASCII" --lnk "$MACCHINA_IMAGES"
  symLink --src "$MACCHINA_HOME" --lnk "$MACCHINA_LINK"
}

if weHave macchina; then
  __init__
else
  exit
fi
